<%= form_for(@setting) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %><br>
    <%= f.label :genre %>
    <%= f.text_field :genre %><br> 
    <%= f.label :high_concept %>
    <%= f.text_field :high_concept %><br>
    <%= f.label :story %>
    <%= f.text_field :story %><br> 
    <input type="hidden" id="writer" name="setting[writer_id]" value=<%= session[:writer_id] %>>
    <%= f.submit %>  
<% end %>
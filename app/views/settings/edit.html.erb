Edit Setting

<%= form_for(@setting) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %><br>
    <%= f.label :genre %>
    <%= f.text_field :genre %><br> 
    <%= f.label :high_concept %>
    <%= f.text_field :high_concept %><br>
    <%= f.label :story %>
    <%= f.text_field :story %><br> 
    <%= f.submit %>  
<% end %>

<%= button_to "Destroy Setting", setting_path(@setting), method: :delete %>

<%= render partial: 'application/errors', locals: {entity: @setting} %>